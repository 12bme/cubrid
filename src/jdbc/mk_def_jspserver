OUTPUT = jspserver.jar

CLASS_DIR = $(JDBC_SRC_DIR)/class_jspserver

JSPSERVER_FLAGS = -DSERVER_SIDE_JDBC

CPP_DEF = -DJDBC_DRIVER_MAJOR_VERSION=$(JDBC_DRIVER_MAJOR_VERSION)	\
	  -DJDBC_DRIVER_MINOR_VERSION=$(JDBC_DRIVER_MINOR_VERSION)	\
	  -DJDBC_DRIVER_PATCH_VERSION=$(JDBC_DRIVER_PATCH_VERSION)	\
	  -DJDBC_DRIVER_SERIAL_NUMBER=$(JDBC_DRIVER_SERIAL_NUMBER)	\
	  $(JSPSERVER_FLAGS)

