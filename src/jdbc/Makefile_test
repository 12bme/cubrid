JDBC_SRC_DIR = .

include $(JDBC_SRC_DIR)/mk_def_test
include $(JDBC_SRC_DIR)/make_common

SRCS =	$(JDBC_SRCS)					\
	$(DRIVER_SRC_DIR)/CUBRIDJdbcInfoTable.java	\

all:	$(CLASS_DIR)/$(DRIVER)

$(CLASS_DIR)/$(DRIVER):	$(SRCS)
	mkdir -p $(CLASS_DIR)
	$(JAVAC) -d $(CLASS_DIR) $(SRCS)
	$(MAKE_JAR)

include obj_rule

clean:
	/bin/rm -rf $(CLASS_DIR)
	touch $(JDBC_SRC_DIR)/*.pjava $(JDBC_SRC_DIR)/*.pjava
