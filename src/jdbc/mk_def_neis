DRIVER = cubridjdbc$(JDBC_DRIVER_MAJOR_VERSION).$(JDBC_DRIVER_MINOR_VERSION).$(JDBC_DRIVER_PATCH_VERSION).$(JDBC_DRIVER_SERIAL_NUMBER)N.jar

