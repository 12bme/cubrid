	  -DJDBC_DRIVER_MINOR_VERSION=$(JDBC_DRIVER_MINOR_VERSION)	\
	  -DJDBC_DRIVER_PATCH_VERSION=$(JDBC_DRIVER_PATCH_VERSION)	\
	  -DJDBC_DRIVER_SERIAL_NUMBER=$(JDBC_DRIVER_SERIAL_NUMBER)	\
	  $(NEIS_FLAGS)

