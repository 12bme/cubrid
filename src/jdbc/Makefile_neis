JDBC_SRC_DIR = .

include $(JDBC_SRC_DIR)/mk_def_neis
include $(JDBC_SRC_DIR)/make_common

SRCS =	$(JDBC_SRCS)

all:	$(CLASS_DIR)/$(DRIVER)

#DEBUG_FLAG =

$(CLASS_DIR)/$(DRIVER):	$(SRCS)
	mkdir -p $(CLASS_DIR)
	$(JAVAC) $(DEBUG_FLAG) -classpath $(JDBC_SRC_DIR)/neis/dbguard.jar:$(JDBC_SRC_DIR)/neis/signgate.jar -d $(CLASS_DIR) $(SRCS)
	$(MAKE_JAR)

include obj_rule

clean:
	/bin/rm -rf $(CLASS_DIR)
	touch $(JDBC_SRC_DIR)/*.pjava $(JDBC_SRC_DIR)/*.pjava
